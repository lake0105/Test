
@{
    ViewBag.Title = "List";
    var newList = ViewBag.data as string[];
    var newlist= ViewData["data"] as string[];
    var cur = (int)ViewBag.id;
    var start = cur - 3;
}

<h2>List</h2>

<ul>
    @foreach (var item in newList)
    {
        <li>@item</li>
    }
</ul>

<ul>
    @for (int i=0;i<newlist.Length;i++)
    {
        <li>@newlist[i]</li>
    }
</ul>

<ul>
    @foreach (var item in Model)
    {
        <li>@item</li>
    }

</ul>

<ul>
    
    @for (int i = start; i < cur + 3; i++)
    {
        <li>
            @Html.ActionLink(string.Format("第{0}页", i), "List", "Home", new { id = i }, null)
        </li>
    }
</ul>